xkb_symbols "fnsymbols-altgrf12" {

    include "pp(basic)"
    name[Group1]= "PinePhone Keyboard Case EN-US with Fn for additional symbols and AltGr for F1-F12";

    // regular:    | regular            | shift                 | altgr		       	| shift+altgr		| fn (kernel)	       	| shift+fn (kernel)	|
    // fn:         | fn	                | shift+fn		| altgr+fn		| shift+altgr+fn      	|
    // Esc
    key <ESC>  { [ Escape,		Escape,			F11,			F11			] };
    // 1
    key <AE01> { [ 1, 			exclam, 		F1,	 		F1,			bar,			exclamdown		] };
    key <FK01> { [ bar,			exclamdown,		NoSymbol,		NoSymbol		] };
    // 2
